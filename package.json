{
  "name": "imposter",
  "displayName": "imPoster",
  "description": "automate your coding",
  "version": "0.0.1",
  "publisher": "MarkusKaihola",
  "repository": "https://github.com/makeri89/autocoder",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:autocoder.start"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autocoder.backspace",
        "title": "Backspace"
      },
      {
        "command": "autocoder.start",
        "title": "Start coding",
        "category": "autocoder"
      }
    ],
    "keybindings": [
      {
        "command": "autocoder.backspace",
        "key": "ctrl+backspace"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4"
  }
}
